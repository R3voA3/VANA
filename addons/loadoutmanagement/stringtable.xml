<?xml Version="1.0" Encoding="utf-8"?>
<Project Name="VANA">
	<Package Name="Mod.ccp">
		<Key ID="STR_VANA_TITLE">
			<English>VANA - Loadout Management</English>
		</Key>
		<Key ID="STR_VANA_OVERVIEW">
			<English>VANA's purpose is to add extra loadout sorting options to the Virtual Arsenal. The modifiction is fully client-sided. The original mod was created by Eathox.This mod is incompatible with other mods that affect the arsenal.</English>
		</Key>
		<Key ID="STR_VANA_ACTIONNAME">
			<English>GitHub</English>
		</Key>
	</Package>
	<Package Name="RscDisplayArsenal">
		<Container Name="Template">
			<Key ID="STR_VANA_TEXTNAME_TEXT">
				<English>Name:</English>
			</Key>
			<Key ID="STR_VANA_BUTTONCREATE_TOOLTIP">
				<English>Create New Tab [Ctrl + N]</English>
			</Key>
			<Key ID="STR_VANA_BUTTONRENAME_TOOLTIP">
				<English>Rename Loadouts/Tabs [Ctrl + E]</English>
			</Key>
		</Container>
		<Container Name="UIPopupControlGroup">
			<Key ID="STR_VANA_TITLE_TEXT">
				<English>VANA didn't initialize</English>
			</Key>
			<Key ID="STR_VANA_TEXT_TEXT">
				<English>Something went wrong in the initialization (You wont be able to open the loadout screen)</English>
			</Key>
			<Key ID="STR_VANA_BUTTONOK_TEXT">
				<English>Confirm</English>
			</Key>
			<Key ID="STR_VANA_CHECKBOXTEXT_TEXT">
				<English>Disable Popup</English>
			</Key>
			<Key ID="STR_VANA_HINTTEXT_TEXT">
				<English>(Loadouts under tabs wont be deleted)</English>
			</Key>
		</Container>
		<Container Name="Notifications">
			<Key ID="STR_VANA_LOADOUT_SAVED">
				<English>Loadout "%1" has been saved</English>
			</Key>
			<Key ID="STR_VANA_LOADOUT_REPLACED">
				<English>Loadout "%1" has been replaced</English>
			</Key>
			<Key ID="STR_VANA_LOADOUT_DELETED">
				<English>Loadout "%1" has been deleted</English>
			</Key>
			<Key ID="STR_VANA_TAB_DELETED">
				<English>Tab "%1" has been deleted</English>
			</Key>
			<Key ID="STR_VANA_DELETE_CONFIRMATION">
				<English>Delete Confirmation</English>
			</Key>
			<Key ID="STR_VANA_DELETE_MESSAGE">
				<English>Delete %1: %2</English>
			</Key>
			<Key ID="STR_VANA_RENAME_CONFIRMATION">
				<English>Rename Confirmation</English>
			</Key>
			<Key ID="STR_VANA_RENAME_MESSAGE">
				<English>Rename %1: %2</English>
			</Key>
		</Container>
	</Package>
</Project>